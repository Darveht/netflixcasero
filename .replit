modules = ["web"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Run IBTV"

[[workflows.workflow]]
name = "Run IBTV"
author = 46302604
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python -m http.server 5000 --bind 0.0.0.0"

[[workflows.workflow]]
name = "Run IBTV Backup"
author = 46426828
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx http-server . -p 5000 -a 0.0.0.0"
